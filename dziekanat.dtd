<!ELEMENT prowadzacy (#PCDATA)>
<!ELEMENT punktyECTS (#PCDATA)>



<!ELEMENT Adresy (adres*)>
<!ELEMENT adres (ulica,nrLokalu,kodPocztowy,miejscowosc)>
<!ELEMENT ulica (#PCDATA)>
<!ELEMENT nrLokalu (#PCDATA)>
